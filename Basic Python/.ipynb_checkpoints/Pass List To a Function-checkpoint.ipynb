   "source": [
    "def count(list):\n",
    "    even=0\n",
    "    odd=0\n",
    "    for i in list:\n",
    "        if i%2==0:\n",
    "            even=even+1\n",
    "        else:\n",
    "            odd=odd+1\n",
    "    return even,odd\n",
    "list=[12,32,21,45,67,80]\n",
    "even,odd=count(list)\n",
    "print(\"Even:{} and Odd:{}\".format(even,odd))\n",
    "        "
   ]
  }
